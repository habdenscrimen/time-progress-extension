{
  "name": "Time Progress",
  "version": "1.0.0",
  "description": "Control your working time - see how much of it has passed.",
  "manifest_version": 2,

  "permissions": ["storage"],

  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },

  "chrome_url_overrides": {
    "newtab": "index.html"
  },

  "browser_action": {
    "default_popup": "popup.html"
  }
}
